@inproceedings{alharthi-idle-games:2018,
  author    = {Alharthi, Sultan A. and Alsaedi, Olaa and Toups Dugas, Phoebe O. and Tanenbaum, Theresa Jean and Hammer, Jessica},
  title     = {Playing to Wait: A Taxonomy of Idle Games},
  year      = {2018},
  isbn      = {9781450356206},
  publisher = {Association for Computing Machinery},
